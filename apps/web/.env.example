# InfluxDB Configuration
# Replace these values with your actual InfluxDB instance details

# InfluxDB Server URL (default: http://localhost:8086)
PUBLIC_INFLUXDB_URL=

# InfluxDB API Token (create from InfluxDB UI: Data > API Tokens)
PUBLIC_INFLUXDB_TOKEN=

# InfluxDB Organization (found in InfluxDB UI: Settings > Organization)
PUBLIC_INFLUXDB_ORG=

# InfluxDB Bucket name (where DHT11 data is stored)
PUBLIC_INFLUXDB_BUCKET=

# Optional: InfluxDB Measurement name (default: dht11_reading)
PUBLIC_INFLUXDB_MEASUREMENT=

# Origin
PUBLIC_ORIGIN=

