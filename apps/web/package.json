    "@tailwindcss/vite": "^4.1.11",
    "@vite-pwa/sveltekit": "^1.0.0",
    "bits-ui": "^2.8.11",
    "clsx": "^2.1.1",
    "layerchart": "2.0.0-next.27",
    "pwa-asset-generator": "^8.0.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "svelte": "^5.36.14",
